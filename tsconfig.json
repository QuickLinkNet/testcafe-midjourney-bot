{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": ".",
    "types": ["node", "testcafe"]
  },
  "include": [
    "discord-automation.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}